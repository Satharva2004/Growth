{
  "expo": {
    "name": "Clarity",
    "slug": "MyApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/unnamed.png",
    "scheme": "Clarity",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/unnamed.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.atharvasawant0804.Clarity",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/unnamed.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.atharvasawant0804.Clarity"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/unnamed.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.1.20",
            "minSdkVersion": 34,
            "compileSdkVersion": 36,
            "targetSdkVersion": 36,
            "enableProguardInReleaseBuilds": true,
            "newArchEnabled": false,
            "packagingOptions": {
              "pickFirst": [
                "**/libc++_shared.so",
                "**/libfbjni.so"
              ]
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c6af951e-af8e-42a8-9127-5e0695b24843"
      }
    }
  }
}